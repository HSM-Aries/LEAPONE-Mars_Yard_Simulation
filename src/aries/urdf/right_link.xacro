<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">




    <!-- Right Rocker Link -->
    <link name="R_Rocker_Link">
        <inertial>
            <origin xyz="-0.0708464812196745 -0.0262779873712159 0" rpy="0 0 0"/>
            <mass value="0.467421555266234"/>
            <inertia
                ixx="0.0013791875014213"
                ixy="-0.00243008562643184"
                ixz="0"
                iyy="0.00977976029683704"
                iyz="0"
                izz="0.0111539619683355"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_Rocker_Link.STL"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_Rocker_Link.STL"/>
            </geometry>
        </collision>
    </link>

    <!-- Right Rocker Joint -->
    <joint name="R_Rocker_Joint" type="revolute">
        <origin xyz="0 -0.271 0.070999999999999" rpy="${PI/2} 0 ${PI}"/>
        <parent link="base_link"/>
        <child link="R_Rocker_Link"/>
        <axis xyz="0 0 -1"/>
        <limit upper="-0.226" lower="0.323" effort="1000" velocity="5"/>
        <xacro:if value="${enable_mimic}">
            <mimic joint="L_Rocker_Joint" multiplier="-1.0" offset="0.0"/>
        </xacro:if>
        <dynamics damping="50" friction="50"/>
    </joint>

    

    <!-- Right Wheel 1 -->
    <link name="R_1_Wheel_Link">
        <inertial>
            <origin xyz="0 0 0.0350608778434713" rpy="0 0 0"/>
            <mass value="${wheel_mass}"/>
            <inertia
                ixx="0.00914288966451803"
                ixy="0"
                ixz="0"
                iyy="0.00914288966451804"
                iyz="0"
                izz="0.0160987749043434"/>
            </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_1_Wheel_Link.STL"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thickness}"/>
            </geometry>
        </collision>
    </link>

    <!-- Right Wheel 1 Joint -->
    <joint name="R_1_Wheel_Joint" type="continuous">
        <origin xyz="0.1542 -0.1266 -0.086" rpy="0 0 0"/>
        <parent link="R_Boggie_Link"/>
        <child link="R_1_Wheel_Link"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="0.01" friction="0.01"/>
    </joint>

    <!-- Right Wheel 2 -->
    <link name="R_2_Wheel_Link">
        <inertial>
            <origin xyz="0 0 0.0350608778434713" rpy="0 0 0"/>
            <mass value="${wheel_mass}"/>
            <inertia
                ixx="0.00914288966451803"
                ixy="0"
                ixz="0"
                iyy="0.00914288966451803"
                iyz="0"
                izz="0.0160987749043434"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_2_Wheel_Link.STL"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thickness}"/>
            </geometry>
        </collision>
    </link>

    <!-- Right Wheel 2 Joint -->
    <joint name="R_2_Wheel_Joint" type="continuous">
        <origin xyz="-0.1267 -0.1545 -0.086" rpy="0 0 0"/>
        <parent link="R_Boggie_Link"/>
        <child link="R_2_Wheel_Link"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="0.01" friction="0.01"/>
    </joint>

    <!-- Right Wheel 3 -->
    <link name="R_3_Wheel_Link">
        <inertial>
            <origin xyz="0 0 0.0350608778434713" rpy="0 0 0"/>
            <mass value="${wheel_mass}"/>
            <inertia
                ixx="0.00914288966451803"
                ixy="0"
                ixz="0"
                iyy="0.00914288966451803"
                iyz="0"
                izz="0.0160987749043434"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_3_Wheel_Link.STL"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thickness}"/>
            </geometry>
        </collision>
    </link>

    <!-- Right Wheel 3 Joint -->
    <joint name="R_3_Wheel_Joint" type="continuous">
        <origin xyz="-0.276 -0.1541 -0.0760000000000001" rpy="0 0 0"/>
        <parent link="R_Rocker_Link"/>
        <child link="R_3_Wheel_Link"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="0.01" friction="0.01"/>
    </joint>

    <!-- Right Boggie Link -->
    <link name="R_Boggie_Link">
        <inertial>
            <origin xyz="0.00949530058774795 -0.0867241425807485 -0.00999999999999995" rpy="0 0 0"/>
            <mass value="1.297654231795819"/>
            <inertia
                ixx="0.000853802912911995"
                ixy="0"
                ixz="0"
                iyy="0.00305171661999932"
                iyz="0"
                izz="0.00390234455443883"/>
            </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_Boggie_Link.STL"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="${mesh_location}/L_Boggie_Link.STL"/>
            </geometry>
        </collision>
    </link>

    <!-- Right Boggie Joint -->
    <joint name="R_Boggie_Joint" type="revolute">
        <origin xyz="0.1411 0.006899 0" rpy="0 0 0"/>
        <parent link="R_Rocker_Link"/>
        <child link="R_Boggie_Link"/>
        <axis xyz="0 0 -1"/>
        <limit lower="-0.560" upper="0.594" effort="10" velocity="100"/>
        <dynamics damping="0.01" friction="0.01"/>
    </joint>

    


    <link name="aux_R_Rocker_Link">
        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="0.0001"/>
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
        </inertial>    
    </link>

    <joint name="aux_R_Rocker_joint" type="revolute">
        <origin xyz="-0.0092 0.06977 0"
                rpy="0 0 0" />
        <parent link="R_Rocker_Link" />
        <child link="aux_R_Rocker_Link" />
        <limit lower="-3.14" upper="3.14" effort="0.0" velocity="5"/>
        <dynamics damping="0.05" friction="0.0"/>
        <axis xyz="0 0 1" />
    </joint>

    

    
    

    
</robot>